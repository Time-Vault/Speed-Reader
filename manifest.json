{
  "manifest_version": 3,
  "name": "Speed Reader",
  "version": "1.0",
  "description": "A simple Chrome extension meant to allow users to read sites quicker.",

  "icons": {
    "16": "icons/SRLogo16.png",
    "32": "icons/SRLogo32.png",
    "48": "icons/SRLogo48.png",
    "128": "icons/SRLogo128.png"
  },

  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ]
}